
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script type="text/javascript" async src="~/Scripts/js/Cart.js"></script>
<div>@ViewBag.sd</div>
<seaction class="bread-crumb">
    <div class="container">
        <div class="title-content-top">
            <h1>Cart</h1>
        </div>
        <div class="row">
            <div class="col-12 text-left">
                <ul class="ul-breadcrumb">
                    <li class="">
                        <a href="/Home" class="home">Home</a>
                        <span>
                            <svg class="w-12px h-12px" fill="currentColor" stroke="currentColor" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 256 512">
                                <path d="M17.525 36.465l-7.071 7.07c-4.686 4.686-4.686 12.284 0 16.971L205.947 256 10.454 451.494c-4.686 4.686-4.686 12.284 0 16.971l7.071 7.07c4.686 4.686 12.284 4.686 16.97 0l211.051-211.05c4.686-4.686 4.686-12.284 0-16.971L34.495 36.465c-4.686-4.687-12.284-4.687-16.97 0z">
                                </path>
                            </svg>
                        </span>
                    </li>
                    <li><span>Cart</span></li>
                </ul>
            </div>
        </div>
    </div>
</seaction>
<main class="main-cart">
    <div class="container">
        <div class="scd__wrapper sf-cart pb-20" data-cart-wrapper="">
            <form action="" method="post" novalidate="" class="cart__form scd__content">
                <div class="cart__table w-full">
                    <div class="cart__table-head flex mb-1">
                        <div class="cart__table-col cart__table-product">Product</div>
                        <div class="cart__table-col cart__table-price">Price</div>
                        <div class="cart__table-col cart__table-quantity hidden-xs block">Quantity</div>
                        <div class="cart__table-col text-right cart__table-subtotal hidden-xs block">Total</div>
                    </div>
                    @{ Html.RenderAction("ListCart", "Cart");}
                </div>
                <div class="flex justify-end">
                    @{ Html.RenderAction("CartSum", "Cart");}
                </div>
            </form>
        </div>
    </div>
</main>
@{ Html.RenderAction("Search", "Home");}